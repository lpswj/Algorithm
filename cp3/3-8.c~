#include <stdio.h>
#include <math.h>
#include "Num.h"

main()
{
		int i;
		point p[3];

		printf("input the point:x,y\n");
		for(i = 0;i < 3;i++)
				scanf("%f %f",&(p[i].x),&(p[i].y));

		if(isLine(p[0],p[1],p[2]) == 1)
				printf("the three points is in one line\n");
		else
				printf("Not in one line\n");

}
